# Configuration for ILIAS-Shell
# It allows for personalized requests against the ILIAS REST Interface. Therefore the OAuth2 - resource owner credentials Authentification mechanism is used.
[Authentification]
api_key=my_key
username=my_username
password=my_passwd
oauth2_endpoint=/v2/oauth2/token

[System]
ilias_client_id=beta
rest_endpoint=http://localhost/ilias5/restplugin.php
